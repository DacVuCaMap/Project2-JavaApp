            <Pane prefHeight="50.0" prefWidth="73.0">
               <children>
                  <Rectangle fx:id="status" arcHeight="5.0" arcWidth="5.0" fill="#38ff1f" height="18.0" layoutY="16.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="53.0" />
               </children>
            </Pane>
            <Button id="btnDetail" fx:id="labelDetail" mnemonicParsing="false" prefHeight="23.0" prefWidth="44.0" text="...">
